<ul>
  <p>Game.id: <%= @game.id %></p>
  <p>Games.rooms.last.id: <%= @game.rooms.last.id %></p>
  <p>Games.rooms.first.id: <%= @game.rooms.first.id %></p>
  <p>Hero.id: <%= @game.hero.id %></p>
  <p>Hero.room: <%= @game.hero.room_number %></p>
</ul>
<br>
<ul>
  <li>
    <% @game.rooms.each do |room| %>
      <p>Id: <%= room.id %> Encounter: <%= room.encounter %> Visited?: <%= room.visited %></p>
    <% end %>
  </li>
</ul>